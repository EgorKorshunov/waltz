{
  "name": "waltz",
  "version": "0.8.0",
  "description": "Waltz controls - lightweight SCADA web frontend",
  "main": "index.html",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "mocha-webpack --require test/setup.js --webpack-config webpack.test.config.js \"test/**/*.test.js\"",
    "lint": "eslint ./",
    "build": "webpack --env.production true",
    "module": "webpack --env.production true --env.module true",
    "standalone": "webpack --env.production true --env.module true --env.standalone true",
    "start": "webpack-dev-server",
    "user-context-server": "node test.server.js",
    "war": "rm -rf dist && node war.js $npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waltz-controls/waltz.git"
  },
  "keywords": [
    "waltz",
    "waltz-controls",
    "tango-controls",
    "web",
    "jive",
    "astor",
    "epics",
    "tine"
  ],
  "author": "Ingvord",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/waltz-controls/waltz/issues"
  },
  "homepage": "https://www.waltz-controls.space",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "archiver": "^3.1.1",
    "atob": "^2.1.2",
    "babel-loader": "^8.1.0",
    "btoa": "^1.2.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.2",
    "express": "^4.17.1",
    "file-loader": "^0.11.2",
    "fs": "0.0.1-security",
    "jsdom": "^11.6.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.0.3",
    "mocha-webpack": "2.0.0-beta.0",
    "null-loader": "^0.1.1",
    "url-loader": "^0.5.9",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@waltz-controls/eventbus": "^1.2.1",
    "codemirror": "^5.52.2",
    "jquery": "^3.4.1",
    "plotly.js-dist": "^1.53.0",
    "webix": "^7.2.0"
  }
}
